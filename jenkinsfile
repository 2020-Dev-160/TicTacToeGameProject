pipeline {
agent any
stages{
stage('Build'){
steps{
echo"Building the code ..."
bat "mvn clean"
}
}
stage('compile'){
steps{
echo"compiling the code ..."
bat "mvn compile"
}
}
stage('Test'){
steps{
echo"Testing the code ..."
bat "mvn test"
}
}
stage('Deploy'){
steps{
echo"Deploying the code ..."
}
}

}
}
